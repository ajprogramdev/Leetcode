import  twoSum from "../DataStructures/Array/Easy/twosum";
describe('Given an array of integers nums and an integer target, return indices of the two numbers such that they add up to target.', 
() => {

    test(' 1- Should return correct array index.', () => {
      expect(twoSum([2,7,11,15], 9)).toEqual([0,1]); 
    });

    test(' 2- Should return correct array index.', () => {       
        expect(twoSum([3,2,4], 6)).toEqual([1,2]);
    });

    test('3- Should return correct array index.', () => {       
        expect(twoSum([3,3], 6)).toEqual([0,1]);
    });


});